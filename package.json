{
  "name": "conventional-changelog-conventionalcommits-factorio",
  "version": "1.1.0",
  "description": "conventional-changelog preset for factorio mod changelogs",
  "main": "index.js",
  "scripts": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/fgardt/conventional-changelog-conventionalcommits-factorio.git"
  },
  "keywords": [
    "conventional-changelog",
    "factorio"
  ],
  "author": "fgardt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fgardt/conventional-changelog-conventionalcommits-factorio/issues"
  },
  "homepage": "https://github.com/fgardt/conventional-changelog-conventionalcommits-factorio#readme",
  "dependencies": {
    "compare-func": "^2.0.0",
    "lodash": "^4.17.15",
    "q": "^1.5.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^20.0.2"
  }
}
